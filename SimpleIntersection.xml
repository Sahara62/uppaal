<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.1//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_2.dtd'>
<nta>
	<declaration>// Place global declarations here.
int cross;</declaration>
	<template>
		<name x="5" y="5">CarCouse</name>
		<declaration>// Place local declarations here.
clock local_clock;</declaration>
		<location id="id0" x="-59" y="76">
			<name x="-85" y="42">A3</name>
		</location>
		<location id="id1" x="-59" y="-17">
			<name x="-85" y="-51">A2</name>
			<label kind="invariant" x="-178" y="-25">local_clock&lt;15</label>
		</location>
		<location id="id2" x="-59" y="-127">
			<name x="-85" y="-161">A1</name>
			<label kind="invariant" x="-178" y="-136">local_clock&lt;20</label>
		</location>
		<location id="id3" x="119" y="-195">
			<name x="109" y="-229">respone</name>
			<label kind="invariant" x="144" y="-204">local_clock&lt;=200</label>
		</location>
		<init ref="id3"/>
		<transition>
			<source ref="id0"/>
			<target ref="id3"/>
			<label kind="assignment" x="-8" y="85">local_clock:=0</label>
			<nail x="144" y="68"/>
			<nail x="161" y="-85"/>
		</transition>
		<transition>
			<source ref="id3"/>
			<target ref="id2"/>
			<label kind="guard" x="-136" y="-204">cross==0 &amp;&amp; local_clock&gt;=100</label>
			<label kind="assignment" x="-119" y="-178">cross:=1,local_clock:=0</label>
		</transition>
		<transition>
			<source ref="id1"/>
			<target ref="id0"/>
			<label kind="guard" x="-59" y="8">local_clock&gt;=5</label>
			<label kind="assignment" x="-59" y="25">local_clock:=0,cross:=0</label>
		</transition>
		<transition>
			<source ref="id2"/>
			<target ref="id1"/>
			<label kind="guard" x="-59" y="-106">local_clock&gt;=10</label>
			<label kind="assignment" x="-59" y="-93">local_clock:=0</label>
		</transition>
	</template>
	<system>// Place template instantiations here.
ns = CarCouse();
ew = CarCouse();
// List one or more processes to be composed into a system.
system ns,ew;
    </system>
	<queries>
		<query>
			<formula>E&lt;&gt; deadlock
			</formula>
			<comment>
			</comment>
		</query>
		<query>
			<formula>A[] not deadlock
			</formula>
			<comment>
			</comment>
		</query>
	</queries>
</nta>
